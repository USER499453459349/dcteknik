{
  "security_headers": {
    "content_security_policy": {
      "default_src": "'self'",
      "script_src": [
        "'self'",
        "'unsafe-inline'",
        "'unsafe-eval'",
        "https://www.googletagmanager.com",
        "https://www.google-analytics.com",
        "https://maps.googleapis.com",
        "https://cdnjs.cloudflare.com",
        "https://cdn.jsdelivr.net",
        "https://unpkg.com"
      ],
      "style_src": [
        "'self'",
        "'unsafe-inline'",
        "https://cdnjs.cloudflare.com",
        "https://cdn.jsdelivr.net",
        "https://fonts.googleapis.com"
      ],
      "img_src": [
        "'self'",
        "data:",
        "https:",
        "blob:"
      ],
      "font_src": [
        "'self'",
        "https://cdnjs.cloudflare.com",
        "https://fonts.gstatic.com",
        "https://cdn.jsdelivr.net"
      ],
      "connect_src": [
        "'self'",
        "https://www.google-analytics.com",
        "https://maps.googleapis.com",
        "https://api.github.com"
      ],
      "media_src": [
        "'self'",
        "https://cdn.coverr.co",
        "https://images.unsplash.com"
      ],
      "frame_src": [
        "'self'",
        "https://www.google.com",
        "https://maps.google.com"
      ],
      "object_src": "'none'",
      "base_uri": "'self'",
      "form_action": "'self'",
      "upgrade_insecure_requests": true
    },
    "permissions_policy": {
      "camera": "()",
      "microphone": "()",
      "geolocation": "()",
      "payment": "()",
      "usb": "()",
      "magnetometer": "()",
      "gyroscope": "()",
      "accelerometer": "()",
      "ambient_light_sensor": "()",
      "autoplay": "()",
      "fullscreen": "()",
      "picture_in_picture": "()"
    },
    "referrer_policy": "strict-origin-when-cross-origin",
    "strict_transport_security": {
      "max_age": 31536000,
      "include_sub_domains": true,
      "preload": true
    }
  },
  "security_monitoring": {
    "xss_protection": true,
    "clickjacking_protection": true,
    "csrf_protection": true,
    "data_leak_protection": true,
    "malicious_script_detection": true,
    "insecure_connection_monitoring": true
  },
  "performance_optimization": {
    "image_optimization": {
      "lazy_loading": true,
      "webp_support": true,
      "avif_support": true,
      "responsive_images": true,
      "compression_quality": 85
    },
    "font_optimization": {
      "preload_critical_fonts": true,
      "font_display": "swap",
      "font_subsetting": true
    },
    "script_optimization": {
      "defer_non_critical": true,
      "async_third_party": true,
      "code_splitting": true,
      "minification": true
    },
    "caching_strategy": {
      "static_assets": "31536000",
      "html_pages": "3600",
      "api_responses": "300",
      "images": "31536000",
      "fonts": "31536000",
      "css": "31536000",
      "js": "31536000"
    },
    "resource_hints": {
      "dns_prefetch": [
        "fonts.googleapis.com",
        "fonts.gstatic.com",
        "cdnjs.cloudflare.com",
        "cdn.jsdelivr.net",
        "unpkg.com"
      ],
      "preconnect": [
        "fonts.googleapis.com",
        "fonts.gstatic.com"
      ],
      "preload": [
        "logo-new.svg",
        "style.css",
        "script.js"
      ]
    }
  },
  "monitoring": {
    "core_web_vitals": {
      "lcp_threshold": 2500,
      "fid_threshold": 100,
      "cls_threshold": 0.1
    },
    "performance_metrics": {
      "load_time_threshold": 3000,
      "render_time_threshold": 1000,
      "memory_usage_threshold": 50
    },
    "security_metrics": {
      "xss_attempts": 0,
      "csrf_attempts": 0,
      "clickjacking_attempts": 0,
      "data_leaks": 0
    }
  },
  "alerts": {
    "performance_alerts": {
      "slow_page_load": true,
      "high_memory_usage": true,
      "large_resource_size": true,
      "slow_third_party": true
    },
    "security_alerts": {
      "xss_attempts": true,
      "csrf_attempts": true,
      "clickjacking_attempts": true,
      "data_leaks": true,
      "malicious_scripts": true
    }
  },
  "compliance": {
    "gdpr": {
      "enabled": true,
      "cookie_consent": true,
      "data_protection": true,
      "user_rights": true
    },
    "ccpa": {
      "enabled": true,
      "privacy_notice": true,
      "opt_out": true
    },
    "wcag": {
      "level": "AA",
      "accessibility": true,
      "keyboard_navigation": true,
      "screen_reader": true
    }
  }
}
